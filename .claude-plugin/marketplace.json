{
  "name": "unify-data-engineering-marketplace",
  "metadata": {
    "description": "Enterprise-grade PySpark data engineering plugin with multi-agent orchestration, intelligent hooks, and Azure Synapse integration",
    "version": "1.0.0",
    "pluginRoot": "."
  },
  "owner": {
    "name": "Linus McMananey",
    "email": "linus.mcmanamey@example.com",
    "url": "https://github.com/linus-mcmanamey"
  },
  "plugins": [
    {
      "name": "unify_2_1",
      "source": ".",
      "description": "Comprehensive Unify 2.1 data migration plugin with multi-agent orchestration, PySpark development, and Azure DevOps integration for medallion architecture ETL pipelines",
      "version": "1.0.0",
      "author": "Linus McMananey",
      "license": "MIT",
      "homepage": "https://github.com/linus-mcmanamey/unify_2_1_plugin",
      "repository": {
        "type": "git",
        "url": "https://github.com/linus-mcmanamey/unify_2_1_plugin.git"
      },
      "keywords": [
        "pyspark",
        "azure",
        "synapse",
        "etl",
        "medallion-architecture",
        "data-engineering",
        "orchestration",
        "multi-agent",
        "devops",
        "hooks",
        "intelligent-routing"
      ],
      "components": {
        "commands": [
          "prime-claude",
          "multi-agent",
          "orchestrate",
          "background",
          "write-tests",
          "code-review",
          "describe",
          "create-feature",
          "explain-code",
          "refactor-code",
          "pyspark-errors",
          "update-docs",
          "create-prd",
          "my-devops-tasks",
          "local-commit",
          "pr-feature-to-staging",
          "pr-staging-to-develop",
          "pr-fix-pr-review",
          "pr-deploy-workflow",
          "pr-review",
          "create-pr",
          "create-pull-request",
          "branch-cleanup",
          "performance-monitoring",
          "setup-docker-containers",
          "dev-agent",
          "ultra-think"
        ],
        "agents": [
          "master-orchestrator",
          "developer-pyspark",
          "code-reviewer",
          "test-engineer",
          "developer-python",
          "developer-sql",
          "developer-azure-engineer",
          "performance-engineer",
          "git-manager",
          "code-documenter",
          "powershell-test-engineer",
          "developer-bash-shell",
          "business-analyst",
          "product-manager",
          "system-architect",
          "security-analyst"
        ],
        "skills": [
          "azure-devops",
          "mcp-code-execution",
          "multi-agent-orchestration",
          "project-architecture",
          "project-commands",
          "pyspark-patterns",
          "schema-reference",
          "skill-creator"
        ],
        "hooks": [
          "skill-activation-prompt",
          "orchestrator-interceptor",
          "combined-prompt-hook"
        ]
      },
      "features": {
        "multi_agent_orchestration": {
          "enabled": true,
          "description": "Coordinate 2-8 specialized agents in parallel or sequential workflows",
          "agents": 16,
          "execution_patterns": ["parallel", "sequential", "hybrid"]
        },
        "intelligent_hooks": {
          "enabled": true,
          "description": "Dual-stage prompt routing with automatic skill activation and complexity-based orchestration",
          "cost_estimation": true,
          "automatic_routing": true
        },
        "pyspark_development": {
          "enabled": true,
          "description": "PySpark ETL development for Azure Synapse medallion architecture",
          "layers": ["bronze", "silver", "gold"],
          "utilities": ["SparkOptimiser", "NotebookLogger", "TableUtilities"]
        },
        "azure_devops_integration": {
          "enabled": true,
          "description": "Work item analysis, pipeline creation, PR management",
          "capabilities": ["user_story_analysis", "deployment_planning", "ci_cd_pipelines"]
        },
        "quality_gates": {
          "enabled": true,
          "description": "Mandatory syntax, linting, formatting, and testing validation",
          "checks": ["syntax_check", "linting", "formatting", "tests"]
        },
        "chain_of_verification": {
          "enabled": true,
          "description": "Systematic verification strategy for all orchestrated workflows",
          "workflow": ["Primary Task", "Generate Output", "Identify Weaknesses", "Cite Evidence", "Revise"]
        }
      },
      "requirements": {
        "claude_code": ">=1.0.0",
        "python": ">=3.10",
        "pyspark": ">=3.3.0"
      }
    }
  ]
}
